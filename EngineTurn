void Movement(void*parameter)
{
    while (true)
    {
        /* code */
    
    
    long distance;

    // Inds√¶t 1 eller 2 xQueue fra ToF 
    xQueueReceive(Distances, &distance, portMAX_DELAY);
    if (distance < 25)
    {
        forhjul.bagud();
        baghjul.bagud();
    }
    else if (distance > 25)
    {
        forhjul.fremad();
        baghjul.fremad();
    }
    else if (leftDistance < 50 && rightDistance > 100)
    {
      forhjul.hojre();
      baghjul.hojre();
    }
    else if (rightDistance < 50 && leftDistance > 100)
    {
      forhjul.venstre();
      baghjul.venstre();
    }
    
    else 
    forhjul.stopmotor ();
    baghjul.stopmotor();
    
    }


}
